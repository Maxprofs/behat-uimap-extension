{
    "name":         "irs/behat-uimap-extension",
    "description":  "Behat's extension that to define mapping from Magento TAF uimap files.",
    "keywords":     ["Magento", "Behat", "TAF", "uimap"],
    "type":         "library",
    "license":      "MIT",

    "authors": [
        {
            "name":   "Vadim Kusakin",
            "email":  "vadim.irbis@gmail.com"
        }
    ],
    
    "require": {
        "php":          ">=5.4.0",
        "behat/behat":  "2.4.*",
        "behat/mink":   "1.4.*",
        "behat/mink-extension": "*",
        "behat/mink-selenium2-driver": "*",
        "zendframework/zend-cache": "2.1.*@dev"
    },
    
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mikey179/vfsStream": "1.2.*"
    },
    
    "autoload": {
        "psr-0": {"Irs\\BehatUimapExtension": "src/"}
    }
}